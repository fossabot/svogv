{
  "name": "svogv",
  "version": "0.4.0",
  "description": "A decorator based approach for model driven forms and some time saving form widgets.",
  "main": "index.js",
  "scripts": {
    "build": "gulp build && gulp demo:build",
    "demoall": "gulp build && gulp demo:build && npm start",
    "start-bt": "http-server ./dist/bootstrap-demo -a localhost -p 3000 -g -c-1 -o",
    "start-mt": "http-server ./dist/material-demo -a localhost -p 3001 -g -c-1 -o",
    "start": "npm run start-bt & npm run start-mt & wait",
    "test": "gulp test",
    "lint": "gulp lint",
    "docs": "gulp docs",
    "codedoc": "npm run codedoc-bootstrap && npm run codedoc-material",
    "codedoc-bootstrap": "./node_modules/.bin/compodoc -p src/boostrap/tsconfig-srcs.json",
    "codedoc-material": "./node_modules/.bin/compodoc -p src/material/tsconfig-srcs.json",
    "setup": "npm install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joergkrause/svogv.git"
  },
  "keywords": [
    "Angular",
    "Reactive",
    "Forms",
    "Validation",
    "Viewmodel",
    "decorators"
  ],
  "author": "Joerg <IsAGeek> Krause",
  "license": "ISC",
